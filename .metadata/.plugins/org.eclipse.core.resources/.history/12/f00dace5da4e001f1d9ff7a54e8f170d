package q.thread;

public class Run {

	//쓰레드 : 프로그램 내애서 실행의 흐름을 가지고 있는 최소 단위
	// 		 main메소드 실행시 메인쓰레드가 실행되는 것이다.
	public static void main(String[] args) {
		Task t1 = new Task();
		Runnable tsak = new MyRunable();
		
		t1.start();
		
		System.out.println("END : " + Thread.currentThread().getName());
		
	
	}

}
